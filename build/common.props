<Project>
    <Import Project="version.props" />
    <PropertyGroup>
        <Product Condition=" '$(Product)' == '' ">SideBySideEditing</Product>
        <Description Condition=" '$(Description)' == '' ">SideBySideEditing</Description>
        <Company>Bartosz Sekuła</Company>
        <Authors>Bartosz Sekuła</Authors>
        <Copyright>© 2025 Bartosz Sekuła</Copyright>
        <PackageProjectUrl>https://github.com/barteksekula/side-by-side-editing</PackageProjectUrl>
        <PackageLicenseUrl>https://raw.githubusercontent.com/barteksekula/side-by-side-editing/master/LICENSE</PackageLicenseUrl>
        <PackageTags>EPiServer Optimizely CMS sidebyside editing</PackageTags>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
        <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
        <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
        <TypeScriptCompileBlocked>true</TypeScriptCompileBlocked>
    </PropertyGroup>
</Project>
